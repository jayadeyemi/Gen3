{
  "Version": "2012-10-17",
  "Statement": {
      "actions"   : [
        "s3:Get*",
        "s3:List*"
      ],
      "effect"    : "Allow",
      "resource" : [
        "arn:aws:s3:::${var.users_bucket_name}", 
        "arn:aws:s3:::${var.users_bucket_name}/${var.config_folder}/*", 
        "arn:aws:s3:::qualys-agentpackage", 
        "arn:aws:s3:::qualys-agentpackage/*"
      ]
  }
}